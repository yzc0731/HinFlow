(define (problem LIBERO_Floor_Manipulation)
  (:domain robosuite)
  (:language Pick the butter and place it in the basket)
    (:regions
      (bin_region
          (:target floor)
          (:ranges (
              (-0.03 0.24 0.02 0.27)
            )
          )
      )
      (target_object_region
          (:target floor)
          (:ranges (
              (-0.15 -0.25 0.0 -0.2)
            )
          )
      )
      (other_object_region_0
          (:target floor)
          (:ranges (
              (0.075 0.0 0.125 0.05)
            )
          )
      )
      (contain_region
          (:target basket_1)
      )
    )

  (:fixtures
    floor - floor
  )

  (:objects
    butter_1 - butter
    basket_1 - basket
    milk_1 - milk
  )

  (:obj_of_interest
    butter_1
    basket_1
  )

  (:init
    (On butter_1 floor_target_object_region)
    (On basket_1 floor_bin_region)
    (On milk_1 floor_other_object_region_0)
  )

  (:goal
    (And (In butter_1 basket_1_contain_region))
  )

)
